generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alumni_employee {
  uuid                String    @unique @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name                String?   @db.VarChar(30)
  password            String?   @db.VarChar(60)
  token               String?   @db.VarChar(255)
  gender              String?   @db.VarChar(2)
  dob                 DateTime? @db.Date
  add1                String?   @db.VarChar(60)
  add2                String?   @db.VarChar(60)
  add3                String?   @db.VarChar(60)
  add4                String?   @db.VarChar(60)
  role                Int
  facebook            String?   @db.VarChar(100)
  twitter             String?   @db.VarChar(100)
  linkedin            String?   @db.VarChar(100)
  whatsapp            String?   @db.VarChar(20)
  blog                String?   @db.VarChar(100)
  contribution        String?
  organization        String?   @db.VarChar(5)
  note                String?
  status              Int
  about_me            String?
  alumni_id           String    @id @default(dbgenerated("('E'::text || nextval('alumni_student_alumni_id_seq'::regclass))"))
  created_at          DateTime  @default(now()) @db.Timestamp(6)
  created_by          Int
  email_id            String?   @db.VarChar(60)
  emp_code            String?   @db.VarChar(25)
  father_name         String?   @db.VarChar(60)
  is_deleted          Int
  mobile_no           String?   @db.VarChar(10)
  organizer_id        Int?
  profile_image       String?   @db.VarChar(30)
  public_url          String?   @db.VarChar(60)
  relationship_status Int?
  state_id            Int
  tc_class            String?   @db.VarChar(6)
  tc_year             String?   @db.VarChar(60)
  updated_at          DateTime? @db.Timestamp(6)
  user_id             String?   @db.VarChar(30)
  wedding_anniversary DateTime? @db.Date
}

model alumni_students {
  uuid                String?   @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  public_url          String?   @db.VarChar(255)
  public_display      String?   @default(dbgenerated("true")) @db.VarChar
  admission_no        String?   @db.VarChar(100)
  name                String    @db.VarChar(255)
  mobile_no           String?   @db.VarChar(20)
  email_id            String?   @db.VarChar(255)
  password            String?   @db.VarChar(255)
  token               String?
  profile_image       String?   @db.VarChar(500)
  gender              String?   @db.VarChar(20)
  dob                 DateTime? @db.Date
  relationship_status String?   @db.VarChar(50)
  wedding_anniversary DateTime? @db.Date
  add1                String?   @db.VarChar(255)
  add2                String?   @db.VarChar(255)
  add3                String?   @db.VarChar(255)
  add4                String?   @db.VarChar(255)
  role                String?   @db.VarChar(100)
  father_name         String?   @db.VarChar(255)
  about_me            String?
  experties           String?
  facebook            String?   @db.VarChar(255)
  twitter             String?   @db.VarChar(255)
  linkedin            String?   @db.VarChar(255)
  whatsapp            String?   @db.VarChar(50)
  blog                String?   @db.VarChar(255)
  tc_year             Int?
  tc_class            String?   @db.VarChar(100)
  contribution        String?
  state_id            Int?
  ro_id               Int?
  school_id           Int?
  user_id             Int?
  note                String?
  status              String?   @default("active") @db.VarChar(50)
  is_deleted          String?   @default(dbgenerated("false")) @db.VarChar
  created_by          Int?
  created_at          DateTime? @default(now()) @db.Timestamp(6)
  updated_at          DateTime? @default(now()) @db.Timestamp(6)
  alumni_id           String    @id @default(dbgenerated("('S'::text || nextval('alumni_student_alumni_id_seq'::regclass))"))
}

